<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="~/images/favicon.ico">

    <title>USAtoBRAZIL</title>

    <!-- Bootstrap -->
    <link href="~/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/icons/flaticon/flaticon.css" rel="stylesheet">

    <!--PLUGINS STYLE-->
    <link href="~/css/plugins/animate.min.css" rel="stylesheet">
    <link href="~/css/plugins/owl.carousel.min.css" rel="stylesheet">

    <!--THEME STYLE-->
    <link href="~/css/main_style.css" rel="stylesheet">
    <link href="~/css/theme_style.css" rel="stylesheet">
    <!--<link href="css/theme_pixa_default.css" rel="stylesheet">-->
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>
<body class="default_theme pageCenter_Style" style="background:url('images/brazil_color.png') no-repeat fixed top right rgba(210,255,218,1);">
    <!--Center Style THEME DIV -->
    <div class="page_wrapper">
        <!-- Header Combo-->
        <header class="header_section header_section_combo comboHeaderNav_StyleLeftRight headerWeb_theme">

            <!--Navigation header_wrap -->
            <div class="header_wrap">
                <div class="container">
                    <div class="nav_combo_block">
                        <div class="Header_logo"><a href="index.html"><img src="~/images/main_logo.png" alt="USAtoBRAZIL" title="USAtoBRAZIL" /></a></div>
                        <!--Nav Box-->
                        <div class="nav-outer">
                            <!-- Main Menu -->
                            <nav class="navigation_combo navbar-expand-md navbar-light">
                                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                                    <ul class="nav_multilabel clearfix">
                                        <li><a href="index.html"><span>Home</span></a></li>
                                        <li><a href="package.html"><span>About Us</span></a></li>
                                        <li><a href="hotels.html"><span>Popular Products</span></a></li>
                                        <li><a href="gifts.html"><span>Contact us</span></a></li>
                                    </ul>
                                </div>
                            </nav>
                            <!-- Main Menu End-->
                        </div>

                        <!--Header Right-->
                        <div class="outer-box">
                            <div class="service_wrapper">
                                @*  <partial name="_LoginPartial"></partial> *@
                                <div class="headerRight_btn">
                                    @if (User.Identity.IsAuthenticated)
                                    {
                                        <a href="/">Hello @User.Identity.Name</a>
                                        <a asp-action="Logout" asp-controller="Home">Logout</a>
                                    }
                                    else
                                    {
                                        <a asp-action="Login" asp-controller="Home"> Sign in</a>
                                        <a asp-action="Register" asp-controller="Home">Sign up</a>
                                    }
                                </div>
                                @* <div class="headerRight_btn">
                                <a href="#">Sign in</a>
                                <a href="#">Sign up</a>
                                </div> *@
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!--End Navigation header_wrap -->
            <!-- Mobile Header -->
            <div class="mobileNavCombo_header">
                <div class="Header_logo"><a href="index.html"><img src="~/images/main_logo.png" alt="USAtoBRAZIL" title="USAtoBRAZIL" /></a></div>
                <!--Nav Box-->
                <div class="nav-outer clearfix">
                    <!--Keep This Empty / Menu will come through Javascript-->
                </div>

            </div>


            <!-- Mobile Menu  -->
            <div class="mobile-menu">
                <span class="mobile-menu-back-drop"></span>
                <div class="menu-outer">
                    <nav class="nav_combo_block">
                        <div class="nav-logo"><a href="index.html"><img src="~/images/main_logo.png" alt="USAtoBRAZIL" title="USAtoBRAZIL" /></a></div><!--Here Menu Will Come Automatically Via Javascript / Same Menu as in Header-->
                    </nav>

                    <div class="service_wrapper">
                        <div class="headerRight_btn">
                            <a href="#">Sign in</a>
                            <a href="#">Sign up</a>
                        </div>
                    </div>
                </div>
            </div><!-- End Mobile Menu -->
        </header>
        <!--End Main Header -->
        <main class="main_container_wrapper">
            <div class="container">
                <div class="row justify-content-center">
                    @RenderBody()
                </div>
            </div>
        </main>


        <footer id="section_panel" class="footer_part">
            <div class="container">
                <div class="">
                    <div class="row">
                        <div class="col-md-4">
                            <div class="footer_logo"><a href="index.html"><img src="~/images/main_logo_ft.png" alt="USAtoBRAZIL" title="USAtoBRAZIL" /></a></div>
                            <div class="footer_content_block">
                                <p>Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled it to make a type specimen book.<p>
                            </div>
                        </div>
                        <div class="col-md-5">
                            <div class="footer_content_block">
                                <h5>Quick Links</h5>
                                <div class="bottom_links_line quicLinks_block">
                                    <div class="quickLink_col">
                                        <a href="#">About us</a>
                                        <a href="#">Popular Products</a>
                                        <a href="#">Contact us</a>
                                    </div>
                                    <div class="quickLink_col">
                                        <a href="#">Privacy Policy</a>
                                        <a href="#">Terms & Conditions</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="footer_content_block">
                                <h5>News Letter</h5>
                                <p>Sign up today for hints, tips and the latest product news</p>
                                <div class="newsletter_inputbox">
                                    <input class="inputNewsEmail" name="" type="text" value="" placeholder="Enter your email">
                                    <button class="formActionBtn"><i class="fa fa-long-arrow-right"></i></button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="copyright_part">
                    <div class="copyright_info">Copyright ©<script>document.write(new Date().getFullYear());</script> USAtoBRAZIL, All rights reserved</div>
                </div>
            </div>
        </footer>
    </div>
    <!--END Center Style THEME DIV -->
    <!--(necessary JavaScript plugins)====================================================================== -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/froala-editor/1.1.9/js/libs/jquery-1.10.2.min.js"></script>
    <script src="~/js/popper.min.js"></script>
    <script src="~/js/bootstrap.min.js"></script>

    <!--PLUGINS-->
    <script src="~/js/plugins/wow.min.js"></script>
    <script src="~/js/plugins/easing.min.js"></script>
    <script src="~/js/plugins/owl.carousel.min.js"></script>
    <script src="~/js/plugins/video.popup.js"></script>

    <script src="~/js/plugins.js"></script>
    <script src="~/js/custom_scripts.js"></script>



    @RenderSection("scripts", required: false)
</body>
</html>